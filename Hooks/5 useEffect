import React, {useState,useEffect} from 'react'

function App() {

  
  const [count, setCount] = useState(0);


  useEffect(() => {
      document.title = 'you have clicked ${count} times';
  });


  return(

        <>
          <button onClick ={()=> setCount(count +1)}> Click </button>
         
        </>
        

    );
}



export default App;
